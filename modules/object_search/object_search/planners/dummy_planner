from planner import Planner

class DummyPlanner(Planner):
    """Dummy planner that does nothing."""
    
    def __init__(self, target_obj_info, args, verbose=True, stateDict=None):
        super().__init__(target_obj_info, args, verbose)
        self.stateDict = stateDict if stateDict is not None else {}
        self.coords = [(0, 0), (100, 100)]  # Example coordinates for subgoals

    def compute_selected_subgoal(self):
        """Returns the first subgoal in the list."""
        if self.new_subgoals:
            return self.new_subgoals[0]
        return None